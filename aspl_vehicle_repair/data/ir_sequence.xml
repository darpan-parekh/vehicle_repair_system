<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="appointment_seq" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">vehicle.appointment</field>
            <field name="prefix">AP</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>

        <record id="vehicle_job_card_seq" model="ir.sequence">
            <field name="name">Vehicle Job Card</field>
            <field name="code">vehicle.job.card</field>
            <field name="prefix">JC</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>

        <record id="work_order_seq" model="ir.sequence">
            <field name="name">Work Order</field>
            <field name="code">vehicle.work.order</field>
            <field name="prefix">WO</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>

    </data>
</odoo>